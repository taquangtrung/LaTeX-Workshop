      "distort",
      "scale=${1:factor}",
      "pretex=${1:code}",
      "apptex=${1:code}",
      "draft",
      "lastpage",
      "angle=${1:degrees}",
      "origin="
    ],
    [
      "inkscapeformat=",
      "inkscapelatex",
      "width=",
