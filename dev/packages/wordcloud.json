{
  "includes": {
    "luamplib": []
  },
  "macros": {
    "wordcloud{}": {
      "snippet": "wordcloud{${1:(${2:word1},${3:weight1});(${4:word2},${5:weight2});...}}"
    },
    "wordcloud[]{}": {
      "snippet": "wordcloud[${2:options}]{${1:(${3:word1},${4:weight1});(${5:word2},${6:weight2});...}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "wordcloudFile{}{}": {
      "snippet": "wordcloudFile{${1:file}}{${2:number of words}}"
    },
    "wordcloudFile[]{}{}": {
      "snippet": "wordcloudFile[${3:options}]{${1:file}}{${2:number of words}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "wordcloudIgnoreWords{}": {
      "snippet": "wordcloudIgnoreWords{${1:word1,${2:word2},...}}"
    }
  },
  "envs": {},
  "options": [],
  "keyvals": [
    [
      "scale=${1:factor}",
      "margin=",
      "rotate=${1:degrees}",
      "usecolor",
      "colors={${1:color1,color2,...}}"
    ]
  ]
}