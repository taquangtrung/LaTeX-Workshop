{
  "includes": {
    "ifpdf": [],
    "pgffor": [],
    "xcolor": [],
    "xkeyval": [],
    "fp-basic": [],
    "graphicx": [],
    "etoolbox": []
  },
  "macros": {
    "scanpage{}": {
      "snippet": "scanpage{${1:imagefile}}"
    },
    "scanpage[]{}": {
      "snippet": "scanpage[${2:options}]{${1:imagefile}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "initviewport{}{}{}{}{}": {
      "snippet": "initviewport{${1:scale}}{${2:llx}}{${3:lly}}{${4:w}}{${5:h}}"
    },
    "whitesq(){}": {
      "snippet": "whitesq(${2:x},${3:y}){${1:scale}}"
    },
    "whitesq(){}[]": {
      "snippet": "whitesq(${3:x},${4:y}){${1:x-scale}}[${2:y-scale}]"
    },
    "whitecirc(){}": {
      "snippet": "whitecirc(${2:x},${3:y}){${1:scale}}"
    },
    "whitecirc(){}[]": {
      "snippet": "whitecirc(${3:x},${4:y}){${1:x-scale}}[${2:y-scale}]"
    },
    "origpgcmd": {},
    "origpgnum": {},
    "putn(){}": {
      "snippet": "putn(${2:x},${3:y}){${1:stuff}}"
    },
    "thegrid{}{}": {
      "snippet": "thegrid{${1:width}}{${2:height}}"
    },
    "fileversion": {},
    "filedate": {}
  },
  "envs": {},
  "options": [
    "nogrid",
    "scale=${1:factor}",
    "llx=",
    "lly=",
    "w=",
    "h="
  ],
  "keyvals": [
    [
      "origpgnum=${1:page number}",
      "origindest",
      "origpgnumoff",
      "dx=",
      "dy=",
      "page=${1:page number}",
      "rot=${1:degrees}",
      "tanrot=${1:tangent}"
    ]
  ]
}